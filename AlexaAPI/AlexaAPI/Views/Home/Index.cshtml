@model AlexaAPI.Models.IndexModel

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
    <script src="~/Scripts/jquery-1.9.1.js"></script>
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="http://www.steamdev.com/snippet/js/jquery.snippet.min.js"></script>
    <link href="http://www.steamdev.com/snippet/css/jquery.snippet.min.css" rel="stylesheet" />
</head>
<body>
    <div class="col-lg-12">
        <h1>Voilà!</h1>
        <div class="box">
            <div class="box-header"></div>
            <div class="box-content"></div>
        </div>
        <div class="row">
            <div class="col-lg-12">
                <h2>Category Browse Response</h2>
                <pre class="xmlCode">@Model.CategoryBrowseData</pre>
            </div>
            <div class="col-lg-12">
                <h2>Category Listings Response</h2>
                <pre class="xmlCode">@Model.CategoryListingsData</pre>
            </div>
            <div class="col-lg-12">
                <h2>Sites Linking In Response</h2>
                <pre class="xmlCode">@Model.SitesLinkingInData</pre>
            </div>
            <div class="col-lg-12">
                <h2>Url Info Response</h2>
                <pre class="xmlCode">@Model.UrlInfoData</pre>
            </div>
        </div>
    </div>
</body>
</html>

<script>
    $("pre.xmlCode").snippet("xml",{style:"greenlcd",collapse:true,startCollapsed:false});
</script>
